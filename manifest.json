{
    "name": "Focus Visualizer",
    "author": "kelsS",
    "version": "1.0.0",
    "description": "A chrome extension that allows the user to set their own custom focus indicator color.",
    "permissions": ["activeTab", "declarativeContent", "scripting"],
    "background": {
      "service_worker": "service-worker.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "manifest_version": 3
}